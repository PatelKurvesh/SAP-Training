<mvc:View controllerName="Practicez1_Login.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true" xmlns="sap.m" xmlns:form="sap.ui.layout.form">
	<App>
		<pages>
			<Page title="{i18n>title}">
					<Label class="t1" text="Please fill the form"></Label>
				<content>
					<form:SimpleForm>
						<form:content>
							
							<Label text="Name"></Label>
							<Input id="name"></Input>
							<Label text="Password"></Label>
							<Input id="pwd" type="Password"></Input>
							<Button text="Submit" id="btn" press="onBtnClick"></Button>
						</form:content>
					</form:SimpleForm>
				</content>
			</Page> 
		</pages>
	</App>
</mvc:View>